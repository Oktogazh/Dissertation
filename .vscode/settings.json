{
  "latex-workshop.linting.chktex.enabled": false,
  "latex-workshop.linting.lacheck.enabled": false,
  "latex-workshop.latex.autoClean.run": "onBuilt",
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": false,
    "scminput": false,
    "latex": false,
    "bibtex": false,
    "tex": false
  },
  "terminal.integrated.env.osx": {
    "PATH": "/Library/TeX/texbin:/usr/bin:/bin:/usr/sbin:/sbin"
  },
  "latex-workshop.latex.option.maxPrintLine.enabled": false,
  "latex-workshop.message.log.show": true,
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "/Library/TeX/texbin/latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ]
    }
  ]
}